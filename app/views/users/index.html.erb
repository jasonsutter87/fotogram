<% @users.each do |user| %>
  Id: <%= user.id %>,
  Name: <%= user.name %>,
  Username: <%= user.username %>,
  Bio: <%= user.bio %>,
  Email: <%= user.email %>,
  Photo_id: <%= user.photo_id %>,
  Photo: <img src="<%= Photo.find_by(id: user.photo_id).url %>" height="75" width="75">,
  <br>
<% end %>
