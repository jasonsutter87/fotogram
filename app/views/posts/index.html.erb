<% @posts.each do |post| %>
Id: <%= post.id %>,
Photo_id: <%= post.photo_id %>,
Photo: <img src="<%= Photo.find_by(id: post.photo_id).url %>" height="75" width="75">,
Filter_id:<%= post.filter_id %>,
Filter_opacity: <%= post.filter_opacity %>,
Description: <%= post.description %>,
Location: <%= post.location %>,
<br>
<% end %>
